SELECT * FROM DOCK_MANIFEST WHERE DOCKINGID IN (SELECT DOCKINGID FROM DOCK_MANIFEST WHERE MANIFESTID<12 AND MANIFESTID>8);
SELECT DISTINCT CONTAINERID, WEIGHT FROM MANIFEST_CONTAINER WHERE CONTAINERID IN (SELECT DISTINCT WEIGHT FROM MANIFEST_CONTAINER WHERE WEIGHT<12 AND CARGOTYPE='Broadcasting');
SELECT DOCKINGID, MANIFESTID FROM DOCK_MANIFEST WHERE ARRIVALTIME IN(SELECT ARRIVALTIME FROM DOCK_MANIFEST WHERE DEPARTURETIME BETWEEN '22/FEB/2017' AND '25/FEB/2017');
