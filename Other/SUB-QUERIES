SELECT SHIP_MANIFEST.LOCATIONID_ORIGIN, SHIP_MANIFEST.LOCATIONID_DESTINATION, MANIFEST_CONTAINER.CARGOTYPE

FROM 

(SHIP_MANIFEST INNER JOIN MANIFEST_CONTAINER ON SHIP_MANIFEST.MANIFESTID=MANIFEST_CONTAINER.MANIFESTID);


SELECT SHIP_MANIFEST.LOCATIONID_ORIGIN, SHIP.SHIPNAME, MANIFEST_CONTAINER.WEIGHT
FROM 
((SHIP_MANIFEST 
INNER JOIN SHIP ON SHIP_MANIFEST.SHIPID=SHIP.SHIPID) 
INNER JOIN MANIFEST_CONTAINER ON SHIP_MANIFEST.MANIFESTID=MANIFEST_CONTAINER.MANIFESTID)

WHERE MANIFEST_CONTAINER.WEIGHT<5 AND SHIP.SHIPCOMPANY='Yang Ming Marine Transport';
